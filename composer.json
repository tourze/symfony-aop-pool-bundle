{
  "name": "tourze/symfony-aop-pool-bundle",
  "description": "AOP连接池",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "symfony/console": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/http-kernel": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/service-contracts": "^3",
    "tourze/symfony-aop-bundle": "~0.0.3",
    "tourze/backtrace-helper": "~0.0.5",
    "tourze/symfony-runtime-context-bundle": "0.0.*",
    "utopia-php/pools": "^0.5",
    "monolog/monolog": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopPoolBundle\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1",
    "maglnet/composer-require-checker": "^4"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopPoolBundle\\Tests\\": "tests"
    }
  }
}