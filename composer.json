{
  "name": "tourze/symfony-aop-pool-bundle",
  "description": "Symfony AOP connection pool bundle for automatic resource pooling",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/backtrace-helper": "1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/php-pools": "0.0.*",
    "tourze/symfony-aop-bundle": "~0.0.4",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-runtime-context-bundle": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopPoolBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopPoolBundle\\Tests\\": "tests"
    }
  }
}